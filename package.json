{
  "name": "appworks-js",
  "version": "16.2.2-alpha.1",
  "main": "dist/appworks.js",
  "types": "index.d.ts",
  "main:min": "dist/appworks.min.js",
  "description": "JavaScript library providing an interface for native bindings provided by AppWorks mobile and desktop clients.",
  "dependencies": {},
  "repository": "https://github.com/opentext/appworks.js",
  "devDependencies": {
    "chai": "^3.5.0",
    "concurrently": "^3.1.0",
    "mocha": "^3.2.0",
    "rollup": "^0.40.2",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "ts-node": "1.7.0",
    "tslint": "^4.0.1",
    "typescript": "^2.0.10",
    "typings": "^2.0.0"
  },
  "scripts": {
    "test": "mocha --compilers ts:ts-node/register test/**.spec.ts test/**/*.spec.ts",
    "test:watch": "mocha --compilers ts:ts-node/register test/**.spec.ts test/**/*.spec.ts --watch",
    "lint": "tslint -c tslint.json ./src/**/*.ts",
    "postinstall": "typings install",
    "prebuild": "npm run test",
    "build": "concurrently \"rollup -c\" \"rollup -c --environment BUILD:min\""
  },
  "author": "OpenText Inc",
  "license": "LICENSE"
}
